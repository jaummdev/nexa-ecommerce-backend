{
    "version": 2,
    "buildCommand": "pnpm prisma generate",
    "installCommand": "pnpm install",
    "outputDirectory": ".",
    "functions": {
        "api/index.ts": {
            "runtime": "@vercel/node@3"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api"
        }
    ]
}