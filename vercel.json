{
    "version": 2,
    "buildCommand": "pnpm install && pnpm prisma generate",
    "installCommand": "pnpm install",
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/server.ts"
        }
    ]
}